{
  "GlobalConfiguration": {
    "RouteIsCaseSensitive": false,
    "QoSOptions": {
      "ExceptionsAllowedBeforeBreaking": 1,
      "DurationOfBreak": 1000,
      "TimeoutValue": 15000
    },
    "RateLimitOptions": {
      "DisableRateLimitHeaders": false,
      "QuotaExceededMessage": "Too many requests.",
      "HttpStatusCode": 429
    },
    "HttpHandlerOptions": {
      "UseTracing": true
    }
  }
}